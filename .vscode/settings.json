{
    "tablegenjmp.includePath": [
        "${env: LLVM_BUILD_PATH}/../mlir/include/",
        "${env: LLVM_BUILD_PATH}/../llvm/include/",
    ],
    "C_Cpp.default.includePath": [
        "${env: LLVM_BUILD_PATH}/../mlir/include/",
        "${env: LLVM_BUILD_PATH}/../llvm/include/",
        "Ch1/include",
        "Ch2/include",
        "Ch3/include",
        "Ch4/include",
        "Ch5/include",
        "Ch6/include",
        "Ch7/include",
    ],
    "C_Cpp.clang_format_sortIncludes": true,
    "clangd.arguments": [
        "--compile-commands-dir=build",
    ],
    "clangd.enable": true,
    "clangd.inactiveRegions.useBackgroundHighlight": true,
    "clangd.path": "clangd",
    "editor.fontLigatures": false,
    "mlir.tablegen_server_path": "${env: LLVM_BUILD_PATH}/bin/tblgen-lsp-server",
    "mlir.server_path": "${env: LLVM_BUILD_PATH}/bin/mlir-lsp-server",
    "mlir.pdll_server_path": "${env: LLVM_BUILD_PATH}/bin/mlir-pdll-lsp-server",
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.default.intelliSenseMode": "clang-x64",
    "C_Cpp.inlayHints.autoDeclarationTypes.showOnLeft": true,
    "C_Cpp.inlayHints.parameterNames.enabled": true,
    "C_Cpp.inlayHints.parameterNames.suppressWhenArgumentContainsName": false,
    "C_Cpp.inlayHints.referenceOperator.enabled": true,
    "C_Cpp.clang_format_path": "clang-format",
    "C_Cpp.formatting": "vcFormat",
    "C_Cpp.vcFormat.indent.accessSpecifiers": true,
    "C_Cpp.vcFormat.indent.braces": true,
    "C_Cpp.vcFormat.indent.caseContentsWhenBlock": true,
    "C_Cpp.vcFormat.indent.caseLabels": true,
    "C_Cpp.vcFormat.indent.gotoLabels": "leftmostColumn",
    "C_Cpp.vcFormat.indent.preserveWithinParentheses": true,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "newLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
    "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
    "C_Cpp.vcFormat.newLine.beforeWhileInDoWhile": true,
    "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,
    "C_Cpp.vcFormat.newLine.scopeBracesOnSeparateLines": true,
    "C_Cpp.vcFormat.space.afterCastCloseParenthesis": true,
    "C_Cpp.experimentalFeatures": "enabled",
    "gnuGlobal.autoUpdate": "Enabled",
    "gnuGlobal.debugMode": "Enabled",
    "gnuGlobal.globalExecutable": "/usr/bin/global",
    "gnuGlobal.gtagsExecutable": "/usr/bin/gtags",
    "gnuGlobal.gtagsForceCpp": "Enabled",
    "gnuGlobal.gtagSkipSymlink": "All",
    "clang.cxxflags": [
        "-std=c++20"
    ],
    "clang.executable": "clang",
    "clang.completion.triggerChars": [

        ".",
        ":",
        ">"
    ],
    "files.associations": {
        "*.pkg": "systemverilog",
        "*.inc": "cpp",
        "memory": "cpp",
        "utility": "cpp",
        "__bit_reference": "cpp",
        "__functional_base": "cpp",
        "__node_handle": "cpp",
        "algorithm": "cpp",
        "bitset": "cpp",
        "chrono": "cpp",
        "deque": "cpp",
        "__memory": "cpp",
        "filesystem": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "limits": "cpp",
        "locale": "cpp",
        "optional": "cpp",
        "random": "cpp",
        "ratio": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "vector": "cpp",
        "array": "cpp",
        "variant": "cpp",
        "__nullptr": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "new": "cpp",
        "stdexcept": "cpp",
        "typeinfo": "cpp",
        "cmath": "cpp",
        "atomic": "cpp",
        "strstream": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "cfenv": "cpp",
        "charconv": "cpp",
        "clocale": "cpp",
        "codecvt": "cpp",
        "complex": "cpp",
        "condition_variable": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "list": "cpp",
        "map": "cpp",
        "set": "cpp",
        "unordered_map": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "source_location": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "fstream": "cpp",
        "iomanip": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "mutex": "cpp",
        "ostream": "cpp",
        "sstream": "cpp",
        "streambuf": "cpp",
        "thread": "cpp",
        "cinttypes": "cpp",
        "typeindex": "cpp"
    }
}